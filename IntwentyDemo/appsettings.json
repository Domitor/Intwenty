    "EnableExternalLogins": false,
    "EnableTwoFactorAuthentication": true,
    "ForceTwoFactorAuthentication": false,
    "EnableEMailVerivication": true,
    "EnableAPIKeyGeneration": true,
    "SupportedLanguages": [
      {

        "Name": "English",
        "Culture": "en-US"
      },
      {
        "Name": "Swedish",
        "Culture": "sv-SE"
      }
    ]

  }
