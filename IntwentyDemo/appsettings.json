{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "IdentityConnection": "Data Source=wwwroot/sqlite/IntwentyDb.db",
    "IntwentyConnection": "Data Source=wwwroot/sqlite/IntwentyDb.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "SystemSettings": {
    "IsDevelopment": true,
    "ReCreateModelOnStartUp": true,
    "IdentityDBMS": 4, //1.MS SQL Server, 2.Maria DB, 3.MySQL, 4.SQLite
    "IntwentyDBMS": 4, //1.MS SQL Server, 2.Maria DB, 3.MySQL, 4.SQLite, 5.MongoDb, 6.LiteDB
    "SiteLanguage": "en-US",
    "AllowExternalLogins": false,
    "AllowMFA": false,
    "ForceMFA": false,
    "EnableSMSMfa": false

  }
}

