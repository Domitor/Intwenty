{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "IntwentySettings": {
    "DefaultConnection": "Data Source=wwwroot/sqlite/IntwentyDb.db",
    "IsDevelopment": true,
    "DefaultConnectionDBMS": "SQLite", // MSSqlServer, MySql, MariaDB, PostgreSQL, SQLite, MongoDb, LiteDb
    "SiteLanguage": "en-US",
    "AllowExternalLogins": false,
    "AllowMFA": false,
    "ForceMFA": false,
    "EnableSMSMfa": false

  }
}

