{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "IntwentySettings": {
    "SiteTitle": "Intwenty Demo",
    "AuthenticatorTitle": "Intwenty Demo",
    "DefaultConnection": "Data Source=wwwroot/sqlite/IntwentyDb.db",
    "ReCreateDatabaseOnStartup": true,
    "SeedDatabaseOnStartUp": true,
    "UseDemoSettings": true,
    "DemoAdminUser": "admin@intwenty.dev",
    "DemoAdminPassword": "thriller",
    "DemoUser": "user@intwenty.dev",
    "DemoUserPassword": "thriller",
    "EnableUserInvites": true,
    "EnableLocalization": true,
    "LocalizationMethod": "UserLocalization", // SiteLocalization, UserLocalization
    "DefaultConnectionDBMS": "SQLite", // MSSqlServer, MySql, MariaDB, PostgreSQL, SQLite, MongoDb, LiteDb
