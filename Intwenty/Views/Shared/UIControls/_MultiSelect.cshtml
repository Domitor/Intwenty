@model IUIComplexBinding
@{
    var validationfield = Model.UIId + "_validationmsg";

}
<div class="form-group">
    <label class="control-label">
        @Model.LocalizedTitle @{ if (Model.Mandatory)
            { <span style="color: red"> *</span> }}
    </label>

    <multislect id="@Model.UIId"
                 name="@Model.UIId"
                 class="form-control form-control-lg" 
                 v-model="@Model.JavaScriptObjectName.@Model.DataTableDbName.@Model.DataColumnDbName"
                 v-bind ="@Model.JavaScriptObjectName.@Model.DataTableDbName"
                 v-bind:textfield="@Model.DataViewColumn2DbName"
                 data-metatype=@Intwenty.Model.UserInterfaceModelItem.MetaTypeMultiSelect
                 data-title="@Model.LocalizedTitle"
                 data-validationfield="@validationfield"
                 data-dbtable="@Model.DataTableDbName"
                 data-dbfield="@Model.DataColumnDbName"
                 data-required="@Model.Mandatory">
        <option v-for="item in valuedomains.VALUEDOMAIN_@Model.DomainName" v-bind:value="item.Code">{{item.Value}}</option>
    </multislect>



</div>